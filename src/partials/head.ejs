<%
  const path = isHome ? '.' : '../..'
-%>
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= page %> | Happy Holidays</title>

  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Nunito:wght@400;700&family=Material+Icons&display=swap" rel="stylesheet">

  <link rel="shortcut icon" href="<%= path %>/images/favicon.ico" type="image/x-icon" id="light-scheme-icon">
  <link rel="shortcut icon" href="<%= path %>/images/favicon_light.ico" type="image/x-icon" id="dark-scheme-icon">

  <link rel="stylesheet" href="<%= path %>/styles/styles.css">
</head>

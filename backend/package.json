{
  "name": "@s19129/backend",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn -r tsconfig-paths/register src/server.ts",
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "lint": "eslint ./src --ext ts",
    "type-check": "tsc --noEmit",
    "sql": "ts-node -r tsconfig-paths/register src/scripts/runSql.ts",
    "sql:create": "npm run sql sql/create.sql",
    "sql:seed": "npm run sql sql/seed.sql",
    "build:frontend": "esbuild src/frontend/*.ts --bundle --target=chrome97,firefox94 --outdir=src/static/scripts",
    "dev:frontend": "npm run build:frontend -- --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@s19192/shared": "file:../shared",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "knex": "^0.95.12",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typed": "^1.2.0",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "esbuild": "^0.14.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1"
  }
}

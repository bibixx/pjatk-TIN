{
  "extends": ["airbnb", "airbnb/hooks", "airbnb-typescript", "plugin:prettier/recommended", "plugin:react/jsx-runtime"],
  "parserOptions": {
    "project": "./frontend/tsconfig.json"
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "import/prefer-default-export": 0,
    "react/require-default-props": 0
  }
}
